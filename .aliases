# .dotfiles repo alias
alias dot='/usr/bin/git --git-dir=$HOME/.dotfiles --work-tree=$HOME'

# For good clipboard support, we want to use vimx if possible.
if [ -e /usr/bin/vim ]; then alias vim='/usr/bin/vimx'; fi

# General Git aliases
alias gs='git status '
alias ga='git add '
alias gb='git branch '
alias gc='git commit '
alias gd='git diff '
alias go='git checkout '

alias got='git '
alias get='git '

alias gcam='git commit -am '
